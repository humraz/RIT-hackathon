
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".MainActivity"
    android:id="@+id/asdas"
    android:orientation="vertical"
    android:background="@color/bootstrap_edittext_disabled">
<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorAccent">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:gravity="center_horizontal">

    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_marginTop="5dp"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        card_view:cardCornerRadius="10dp"
        card_view:cardElevation="5dp"
        card_view:cardUseCompatPadding="true"
        android:padding="15dp">
        <com.skyfishjy.library.RippleBackground
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/content"
            app:rb_color="#ff8187"
            app:rb_radius="32dp"
            app:rb_rippleAmount="4"
            app:rb_duration="3000"
            app:rb_scale="6"
            android:layout_gravity="center_horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
            <ImageView
                android:id="@+id/imageView2"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/help"
                android:onClick="soshelpp"
                android:padding="15dp"
                android:layout_margin="10dp"
                android:layout_width="150dp"
                android:layout_height="150dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:text="Help Me"
                    android:layout_gravity="center_horizontal" />
            </LinearLayout>
        </com.skyfishjy.library.RippleBackground>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_xcvview"
        android:layout_marginTop="5dp"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        card_view:cardCornerRadius="10dp"
        card_view:cardElevation="5dp"
        card_view:cardUseCompatPadding="true"
        android:padding="15dp">
        <com.skyfishjy.library.RippleBackground
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/content2"
            app:rb_color="#ff8187"
            app:rb_radius="32dp"
            app:rb_rippleAmount="4"
            app:rb_duration="3000"
            app:rb_scale="6"
            android:layout_gravity="center_horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
            <ImageView
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:id="@+id/imageView22"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/amb"
                android:onClick="calamb"
                android:padding="15dp"
                android:layout_margin="10dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="Call Ambulance"
                android:layout_gravity="center_horizontal" />
  </LinearLayout>

        </com.skyfishjy.library.RippleBackground>
    </android.support.v7.widget.CardView>
</LinearLayout>
    <android.support.v7.widget.CardView
        android:id="@+id/card_view3"

        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        card_view:cardCornerRadius="10dp"
        card_view:cardElevation="5dp"
        card_view:cardUseCompatPadding="true">
        <LinearLayout android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:paddingLeft="9dp">

            <ImageView
                android:id="@+id/imageViews2"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/pb"
                android:layout_height="75dp"
                android:layout_width="75dp" />
            <LinearLayout android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:paddingRight="5dp"
                android:paddingLeft="5dp">


                <com.beardedhen.androidbootstrap.BootstrapButton
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"

                    app:bootstrapBrand="danger"
                    app:bootstrapSize="lg"
                    app:bootstrapText="Set Up Your Contacts"
                    app:buttonMode="regular"
                    app:showOutline="false"
                    app:roundedCorners="true"
                    android:layout_gravity="center_horizontal"

                    android:onClick="con"
                    android:layout_height="35dp"
                    />

            </LinearLayout>
        </LinearLayout>

    </android.support.v7.widget.CardView>




    <android.support.v7.widget.CardView
        android:id="@+id/card_view4"

        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        card_view:cardCornerRadius="10dp"
        card_view:cardElevation="5dp"
        card_view:cardUseCompatPadding="true">
        <LinearLayout android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingTop="5dp"
            android:paddingLeft="5dp">



                <com.beardedhen.androidbootstrap.BootstrapButton
                    android:layout_marginTop="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"

                    app:bootstrapBrand="primary"
                    app:bootstrapSize="lg"
                    app:bootstrapText="Show Local News"
                    app:buttonMode="regular"
                    app:showOutline="false"
                    app:roundedCorners="true"
                    android:layout_gravity="center_horizontal"

                    android:onClick="con2"
                    android:layout_marginRight="5dp" />

            <com.beardedhen.androidbootstrap.BootstrapButton
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="40dp"

                app:bootstrapBrand="primary"
                app:bootstrapSize="lg"
                app:bootstrapText="Forum"
                app:buttonMode="regular"
                app:showOutline="false"
                app:roundedCorners="true"
                android:layout_gravity="center_horizontal"

                android:onClick="con3"
                android:layout_marginRight="5dp" />
        </LinearLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_view544"

        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        card_view:cardCornerRadius="10dp"
        card_view:cardElevation="5dp"
        card_view:cardUseCompatPadding="true">
        <LinearLayout android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingTop="5dp"
            android:paddingLeft="5dp">

            <com.beardedhen.androidbootstrap.BootstrapButton
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="40dp"

                app:bootstrapBrand="primary"
                app:bootstrapSize="lg"
                app:bootstrapText="Find Nearest"
                app:buttonMode="regular"
                app:showOutline="false"
                app:roundedCorners="true"
                android:layout_gravity="center_horizontal"

                android:onClick="con4"
                android:layout_marginRight="5dp" />


        </LinearLayout>

    </android.support.v7.widget.CardView>



    <android.support.v7.widget.CardView
        android:id="@+id/card_view33"

        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        card_view:cardCornerRadius="10dp"
        card_view:cardElevation="5dp"
        card_view:cardUseCompatPadding="true"
        android:layout_gravity="center_horizontal">

        <LinearLayout android:layout_height="match_parent"
            android:layout_width="wrap_content"
            android:orientation="horizontal"

            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:paddingLeft="5dp"
            android:layout_gravity="center_horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginRight="13dp">
                <ImageView
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/imageViews342"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/ambulance"
                    android:onClick="ambulance" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Ambulance"
                    android:id="@+id/textView"
                    android:layout_gravity="center_horizontal" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"

                android:layout_marginRight="13dp">
                <ImageView
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/imageViews32"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/police"
                    android:onClick="police" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Police"
                    android:id="@+id/textVieww"
                    android:layout_gravity="center_horizontal" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"

                android:layout_marginRight="13dp">
                <ImageView
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/imageViews24"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/extinguisher"
                    android:onClick="fire" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Fire"
                    android:id="@+id/textViaeww"
                    android:layout_gravity="center_horizontal" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"

                android:layout_marginRight="13dp">
                <ImageView
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:id="@+id/imageVieasdws24"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/cone"
                    android:onClick="child" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Traffic"
                    android:id="@+id/textViaaeww"
                    android:layout_gravity="center_horizontal" />
            </LinearLayout>
        </LinearLayout>

    </android.support.v7.widget.CardView>

</LinearLayout>


</ScrollView>
